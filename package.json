      "\\.js$": "<rootDir>/node_modules/babel-jest",
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
